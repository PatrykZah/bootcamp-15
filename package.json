{
  "name": "git-15",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch": "npm run clean && parcel src/index.html --open --no-autoinstall --port 8080",
    "prebuild": "npm run clean",
    "build": "parcel build src/index.html",
    "clean": "rimraf dist && rimraf .cache",
    "code-quality": "npm run format && npm run lint",
    "format": "prettier --write \"src/**/*.{js,html,scss}\"",
    "lint": "eslint src/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "mustache": "^3.0.1",
    "parcel-bundler": "^1.12.3",
    "posthtml-modules": "^0.4.2",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0",
    "rimraf": "^2.6.3",
    "sass": "^1.19.0",
    "sortablejs": "^1.9.0"
  },
  "dependencies": {
    "dom-parser": "^0.1.6",
    "jquery": "^3.4.0",
    "latest-version": "^5.1.0",
    "popper.js": "^1.15.0",
    "update-v8": "^3.5.3"
  }
}
